<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
  PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="kr.green.leather.dao.MemberDAO">
  <select id="getMember" resultType="kr.green.leather.vo.MemberVO">
    select * from member where member_id = #{member_id};
  </select>
  <insert id="signup">
  	insert into member(member_id,member_pw,member_name,member_phone,member_address,member_email)
  		value (#{member_id},#{member_pw},#{member_name},#{member_phone},#{member_address},#{member_email});
  </insert>
</mapper> 